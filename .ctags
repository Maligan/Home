--langdef=actionscript
--langmap=actionscript:.as
--regex-actionscript=/[ \t]*class[ \t]+([A-Za-z0-9_]+)[ \t]*/\1/c,class, classes/
--regex-actionscript=/^[ \t]*(private|public|protected|internal|static)[ \t]*var[ \t]+([A-Za-z0-9_]+)[ \t]*/\2/v,variable, variables/
--regex-actionscript=/^[ \t]*(private|public|protected|internal|static)[ \t]+function[ \t]+(set|get) [ \t]+([A-Za-z0-9_]+)[ \t]*\(/\2 \2/p,property, properties/
--regex-actionscript=/^[ \t]*(private|public|protected|internal|static)[ \t]+function[ \t]+([A-Za-z0-9_]+)[ \t]*\(/\2/f, function, functions/

